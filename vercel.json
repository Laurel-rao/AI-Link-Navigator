{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "VERCEL": "1",
    "EDGE_CONFIG_ID": "@edge-config-id",
    "EDGE_CONFIG_TOKEN": "@edge-config-token"
  }
} 